<%= simple_form_for @cook do |f| %>
  <%= f.input :speciality %>
  <%= f.input :nickname %>
  <%= f.input :price %>
  <%= f.input :address %>
  <%= f.input :cooker_photo, as: :attachinary %>
  <%= f.input :recipe_photos, as: :attachinary %>
  <%= f.input :location_photos, as: :attachinary %>
  <%= f.submit "add a cook", class: "btn btn-primary" %>
<% end %>
