<h1>Cooks#new</h1>
<p>Find me in app/views/cooks/new.html.erb</p>

<%= simple_form_for @cook do |f| %>
  <%= f.input :speciality %>
  <%= f.input :nickname %>
  <%= f.input :price %>
   <%= f.input :address %>
  <%= f.input :photos_recipes, as: :attachinary %>
  <%= f.input :photos_location, as: :attachinary %>
  <%= f.submit "add a cook", class: "btn btn-primary" %>
<% end %>


<%= content_for(:after_js) do %>
  <script src="https://maps.googleapis.com/maps/api/js?libraries=places&amp;&key=#{ENV['GOOGLE_API_BROWSER_KEY']}"></script>
<% end %>
